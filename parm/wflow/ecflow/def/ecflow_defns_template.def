extrn /totality_limit:TOTALITY
extrn

suite prod_{{ NET }}
  inlimit /totality_limit:TOTALITY
  family {{ NET }}
    edit model_ver '{{ model_ver }}'
    edit PACKAGEHOME '{{   }}'
    edit NET '{{ NET }}'
    edit RUN '{{ RUN }}'
    edit PROJ '{{   }}'
    edit PROJENVIR '{{   }}'
    edit MACHINE_SITE '{{   }}'
    edit ENVIR '{{ envir  }}'
    edit QUEUE '{{ QUEUE_DEFAULT }}'
    edit QUEUE_ARCH '{{ QUEUE_FCST }}'
    edit OUTPUTDIR '{{   }}'
    family {{ cyc }}
      edit CYC '{{ cyc }}'
      edit ECF_FILES '{{ EXPTDIR }}/ecf/scripts'
      family {{ task_group }}
        task 
          trigger
      endfamily # task_group
    endfamily # cyc
  endfamily  # NET
endsuite
